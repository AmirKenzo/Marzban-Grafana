            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "WITH LatestConnection AS (\r\n    SELECT \r\n        user_id, \r\n        node_id,\r\n        ROW_NUMBER() OVER (PARTITION BY user_id ORDER BY created_at DESC) AS row_num\r\n    FROM node_user_usages \r\n    WHERE DATE(created_at) = CURDATE()\r\n)\r\n\r\nSELECT \r\n    IFNULL(n.name, 'Main') AS node,\r\n    COUNT(u.id) AS users\r\nFROM nodes n\r\nLEFT JOIN LatestConnection lc ON n.id = lc.node_id AND lc.row_num = 1\r\nLEFT JOIN users u ON u.id = lc.user_id AND u.online_at >= DATE_SUB(NOW(), INTERVAL 1 MINUTE) AND u.admin_id = ${adminID}\r\nGROUP BY n.name\r\nORDER BY users DESC;\r\n",
            "refId": "A",
